# Azure Blob Storage connection string
AZURE_BLOB_CONNECTION_STRING=<your_blob_connection_string>

# Azure Cognitive Search service name and admin key
AZURE_SEARCH_SERVICE_NAME=<your_search_service_name>
AZURE_SEARCH_ADMIN_KEY=<your_search_admin_key>

# Azure OpenAI service endpoint and key
AZURE_OPENAI_ENDPOINT=<your_openai_endpoint>
AZURE_OPENAI_API_KEY=<your_openai_api_key>

# Azure Form Recognizer endpoint and key
AZURE_FORM_RECOGNIZER_ENDPOINT=<your_form_recognizer_endpoint>
AZURE_FORM_RECOGNIZER_API_KEY=<your_form_recognizer_api_key>

# Redis connection string
REDIS_CONNECTION_STRING=<your_redis_connection_string>

# Other environment variables
ENVIRONMENT=development
CACHE_TTL=300